{
  "name": "swoft-fork/server",
  "type": "library",
  "version": "v2.0.11",
  "keywords": [
    "php",
    "swoole",
    "swoft"
  ],
  "description": "swoft base server component",
  "homepage": "https://github.com/sky-up/swoft-server",
  "license": "Apache-2.0",
  "require": {
    "swoft-fork/stdlib": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Server\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Server\\Unit\\": "test/unit/",
      "SwoftTest\\Server\\Testing\\": "test/testing/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
